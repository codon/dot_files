[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[color]
	ui = true
[branch]
	autosetupmerge = true
[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = $HOME/.gitignore
[alias]
	st = status
	stat = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	dif = diff
	lg = log -p
	up = pull
